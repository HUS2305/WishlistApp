{
  "expo": {
    "name": "Wishly",
    "slug": "wishlistapp",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wishly.app",
      "buildNumber": "1",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIStatusBarStyle": "UIStatusBarStyleDefault",
        "NSCameraUsageDescription": "Wishly needs camera access to capture product photos for your wishlists.",
        "NSPhotoLibraryUsageDescription": "Wishly needs photo library access to add images to your wishlist items.",
        "NSFaceIDUsageDescription": "Wishly uses Face ID for secure authentication."
      }
    },
    "android": {
      "package": "com.wishly.app"
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#C41E68",
      "androidMode": "default",
      "androidCollapsedTitle": "Wishly"
    },
    "plugins": [
      "expo-dev-client",
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#C41E68"
        }
      ],
      "@react-native-community/datetimepicker",
      "expo-secure-store",
      "expo-web-browser",
      "@sentry/react-native"
    ],
    "scheme": "wishly",
    "extra": {
      "eas": {
        "projectId": "e0041232-b612-43a3-b917-2a78c40c023b"
      },
      "router": {},
      "cloudinaryCloudName": "dworagviw"
    },
    "owner": "huss0194"
  }
}
